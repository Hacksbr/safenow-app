{
  "expo": {
    "name": "SafeNow",
    "slug": "safenow",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "githubUrl": "https://github.com/SafeNow/safenow_app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E6E7E8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.safenow.safenow"
    },
    "android": {
      "package": "com.safenow.safenow",
      "versionCode": 1
    }
  }
}
